#version 330 core

//Other Stuff
uniform sampler2D diffuse;
uniform sampler2D normals;
uniform sampler2D specular;

//Input Format
in vec3 ex_Normals;
in vec2 ex_TextureUVs;

//Output Format
out vec4 out_Colour;
 
void main() {
	out_Colour = texture2D(diffuse, ex_TextureUVs);
	
	if(out_Colour.a < 0.9) {
		discard;
	}
}